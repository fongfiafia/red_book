{
    "name": "Chrome Extension Sample",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "Chrome Extension Sample",
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "dist/main.js"
            ],
            "css": [
                "bootstrap/self.css",
                "content_script.css"
            ]
        }
    ]
}